<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="bpadapted"
    nsURI="http://architecture.muebles.losalpes.com.co/bpaAdapted" nsPrefix="bpadapted">
  <eClassifiers xsi:type="ecore:EClass" name="MacroProcess" eSuperTypes="#//BPAEntity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//Process" eOpposite="#//Process/parent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="flowFrom" eType="#//MacroProcess"
        eOpposite="#//MacroProcess/flowTo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="flowTo" eType="#//MacroProcess"
        eOpposite="#//MacroProcess/flowFrom"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Activity" eSuperTypes="#//BPAEntity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignedTo" upperBound="-1"
        eType="#//Role" eOpposite="#//Role/assignedTo"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Process" eSuperTypes="#//BPAEntity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="consistsOf" upperBound="-1"
        eType="#//Activity" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parent" lowerBound="1"
        eType="#//MacroProcess" eOpposite="#//MacroProcess/children"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Role" eSuperTypes="#//BPAEntity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignedTo" upperBound="-1"
        eType="#//Activity" eOpposite="#//Activity/assignedTo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="belongsTo" lowerBound="1"
        eType="#//OrganizationalUnit" eOpposite="#//OrganizationalUnit/has"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="isAsigned" upperBound="-1"
        eType="#//Person" eOpposite="#//Person/has"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OrganizationalUnit" eSuperTypes="#//BPAEntity">
    <eStructuralFeatures xsi:type="ecore:EReference" name="owns" lowerBound="1" upperBound="-1"
        eType="#//MacroProcess" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="has" lowerBound="1" upperBound="-1"
        eType="#//Role" containment="true" eOpposite="#//Role/belongsTo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="boss" eType="#//Person"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Organization">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//BPAEntity" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Person" eSuperTypes="#//BPAEntity">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="completedName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="has" upperBound="-1" eType="#//Role"
        eOpposite="#//Role/isAsigned"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BPAEntity" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="entityname" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="entitydescription" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
